   "cell_type": "code",
   "execution_count": 385,
   "metadata": {},
   "outputs": [],
   "source": [
    "from experiments import rsltdict"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 386,
   "metadata": {},
   "outputs": [],
   "source": [
    "\n",
    "class rsltdict(dict):\n",
    "\t\"\"\"For frozen set items keys (dict keys), want to query results in any order\"\"\"\n",
    "\t__getattr__ = dict.get\n",
    "\t__setattr__ = dict.__setitem__\n",
    "\t__delattr__ = dict.__delitem__\n",
    "\n",
    "\tdef query(self, q = '', **kwargs):  \n",
    "\t\tmax_score = (0,0) #kwarg matches, max arg similarity, lexicographically ordered.\n",
    "\t\targ_max = None\n",
    "\t\tfor k,v in self.items():\n",
    "\t\t\tmatchlength = max(  \\\n",
    "\t\t\t \tSequenceMatcher(None, q, str(v)).find_longest_match(0, len(q), 0, len(str(v))).size \\  \n",
    "\t\t\t\t\tfor v in  dict(k).values() )\n",
    "\t\t\tscore = (len(set(kwargs.items()) & k), matchlength)\n",
    "\t\t\t\n",
    "\t\t\tif score > max_score:\n",
    "\t\t\t\tmax_score = score\n",
    "\t\t\t\targ_max = v\t\t\t\n",
    "\t\t\t\n",
    "\t\treturn arg_max"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 387,
   "metadata": {},
   "outputs": [],
   "source": [
    "from experiments import rsltdict"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 388,
   "metadata": {},
   "outputs": [],
   "source": [
    "from experiments import rsltdict"
